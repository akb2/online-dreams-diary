{
  "name": "online-dreams-map",
  "version": "1.0.0",
  "repository": "https://github.com/akb2/online-dreams-diary",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production --aot --build-optimizer",
    "bundle-stats": "npm run build --stats-json && webpack-bundle-analyzer ../app/stats.json",
    "watch": "npm run build --watch --configuration development",
    "test": "ng test",
    "stop-server": "httpd -k stop | mysqld -u root shutdown",
    "dev-start": "concurrently \"mysqld --console\" \"httpd -k start\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^13.0.3",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@brakebein/threeoctree": "^2.0.1",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-classic": "^31.1.0",
    "ng-recaptcha": "^9.0.0",
    "petrovich": "^0.2.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-devkit/build-optimizer": "^0.1302.1",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.37",
    "@types/three": "^0.135.0",
    "concurrently": "^7.2.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "three": "^0.135.0",
    "three-sky": "^1.1.3",
    "typescript": "~4.8.4"
  }
}