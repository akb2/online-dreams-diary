    "@angular/forms": "^15.1.1",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-classic": "^31.1.0",
    "@ctrl/ngx-emoji-mart": "^7.1.0",
    "@iplab/ngx-color-picker": "^15.0.2",
    "css-calc-transform": "^1.0.0",
    "fabric": "^5.3.0",
    "ng-recaptcha": "^11.0.0",
    "ngx-fabric-wrapper": "^14.0.0",
    "petrovich": "^0.2.1",
    "rxjs": "~7.4.0",
    "snowball-stemmers": "^0.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-devkit/build-optimizer": "^0.1302.1",
    "@angular/cli": "^15.1.2",
    "@angular/compiler-cli": "^15.1.1",
    "@brakebein/threeoctree": "^2.0.1",
    "@types/fabric": "^5.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.37",
    "@types/three": "^0.143.0",
    "concurrently": "^7.2.1",
    "cypress": "^13.1.0",
    "install": "^0.13.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
