{
  "name": "online-dreams-diary-frontend",
  "version": "1.0.0",
  "repository": "https://github.com/akb2/online-dreams-diary",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:docker": "ng serve --poll=2000 --host 0.0.0.0 --disable-host-check",
    "start:docker:ssl": "npm run start:docker -- --ssl true --ssl-key /etc/ssl/private/localhost.key --ssl-cert /etc/ssl/certs/localhost.crt",
    "build": "ng build --configuration=production --aot --build-optimizer",
    "cypress-build": "ng build --configuration=cypress",
    "bundle-stats": "npm run build --stats-json && webpack-bundle-analyzer ../app/stats.json",
    "watch": "npm run build --watch --configuration=development",
    "test": "ng test",
    "stop-server": "httpd -k stop | mysqld -u root shutdown",
    "dev-start": "concurrently \"mysqld --console\" \"httpd -k start\" \"npm run start\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:console:run": "cypress run --reporter junit --reporter-options mochaFile=cypress/results/results-[hash].xml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/cdk": "^15.1.1",
    "@angular/common": "^15.1.1",
    "@angular/compiler": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.1.1",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "@ctrl/ngx-emoji-mart": "^7.1.0",
    "@iplab/ngx-color-picker": "^15.0.2",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "fabric": "^5.3.0",
    "ng-recaptcha": "^11.0.0",
    "ngx-fabric-wrapper": "^14.0.0",
    "noisejs": "^2.1.0",
    "petrovich": "^0.2.1",
    "rxjs": "^7.8.0",
    "sh": "^0.0.3",
    "snowball-stemmers": "^0.6.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@akb2/math": "^1.0.0",
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-devkit/build-optimizer": "^0.1302.1",
    "@angular/cli": "^15.1.2",
    "@angular/compiler-cli": "^15.1.1",
    "@brakebein/threeoctree": "^2.0.1",
    "@ngrx/store-devtools": "^15.4.0",
    "@types/cypress": "^1.1.3",
    "@types/fabric": "^5.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.37",
    "@types/three": "^0.159.0",
    "concurrently": "^7.2.1",
    "cypress": "^13.1.0",
    "install": "^0.13.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm": "^9.6.5",
    "postprocessing": "^6.33.4",
    "three": "^0.159.0",
    "typescript": "^4.9.3"
  }
}