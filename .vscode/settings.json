{
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git/": true,
    "**/.svn/": true,
    "**/.hg/": true,
    "**/CVS/": true,
    "**/node_modules/": true,
    "**/.angular/": true,
    "app/": true,
    "email-templates/": true,
    "db_backup/": true,
    "media/images": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/package-lock.json": true
  },
  "typescript.tsdk": "client\\node_modules\\typescript\\lib",
  "files.associations": {
    "*.ts": "typescript"
  },
  "i18n-ally.displayLanguage": "ru",
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.enabledFrameworks": [
    "ngx-translate"
  ],
  "i18n-ally.localesPaths": [
    "client/src/assets/i18n",
    "client/src/assets/images/icons/languages"
  ],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "ru"
}